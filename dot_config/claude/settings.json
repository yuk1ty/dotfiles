{
  "permissions": {
    "allow": [
      "Read(**)",
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(cat:*)",
      "Bash(echo:*)",
      "Bash(touch:*)",
      "Bash(mkdir:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(cargo:*)",
      "Bash(npm:*)"
    ],
    "deny": [
      "Bash(sudo:*)",
      "Bash(rm:*)",
      "Bash(rm -rf:*)",
      "Bash(git push:*)",
      "Bash(git reset:*)",
      "Bash(git rebase:*)",
      "Read(id_rsa)",
      "Read(id_ed25519)",
      "Read(**/.ssh/*)"
    ]
  },
  "cleanupPeriodDays": 90,
  "preferredNotifChannel": "terminal_bell",
  "theme": "light"
}
